@model MyApp.Quizzmodel.Quizmodel

<p>@Model.Subject Score</p>
    <p>Your Score = @Model.Score / @Model.Questions.Count</p>

    @for (int i=0;i<Model.Questions.Count;i++)
    {
        <p>Your Answer for " @Model.Questions[i].Question " is @Model.Questions[i].UserOption</p>
        <p> The Correct answer is " @Model.Questions[i].CorrectAnswer "</p><br><br>
    }
